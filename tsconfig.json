{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "typeRoots": [      "./types/index.d.ts",
      "./types/vue-shim.d.ts"],
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxtjs/auth",
      "@aframe/types",
      "@three/types",
      "@math-three/types"
    ]
  },
  "noImplicitAny": false,
  "exclude": [
    "node_modules"
  ]
}
