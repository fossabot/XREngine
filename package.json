  "engines": {
    "yarn": ">= 0.18.0"
  },
  "jest": "ts-jest",
  "scripts": {
    "build-client": "tsc -p tsconfig.client.json && npm run copyfiles-client && next build ./lib/client",
    "test": "NODE_ENV=development jest && yarn run lint && yarn run compile",
    "lint": "node node_modules/.bin/eslint --ext .js,.ts server/ --fix",
    "dev": "NODE_ENV=development node --max_old_space_size=3072 -r ts-node/register ./server/",
    "dev-with-spoke": "yarn run build-spoke && NODE_ENV=development node --max_old_space_size=3072 -r ts-node/register ./server/",
    "dev-reinit-db": "NODE_ENV=development FORCE_DB_REFRESH=true node --max_old_space_size=3072 -r ts-node/register ./server/",
    "dev-generate-fakes": "NODE_ENV=development GENERATE_FAKES=true node --max_old_space_size=3072 -r ts-node/register ./server/",
    "start": "yarn run compile && node lib/server/",
    "compile": "shx rm -rf lib/ && tsc && npm run copyfiles-server && npm run build-client",
    "copyfiles-server": "copyfiles server/email-templates/**/* lib",
    "copyfiles-client": "copyfiles client/design/** client/design/**/* client/public/** client/public/**/* client/**/*.scss lib",
    "sync": "PERFORM_DRY_RUN=true FORCE_DB_REFRESH=true node lib/server/",
    "precommit": "no-master-commits -b master,dev",
    "build-spoke": "cd scripts && bash sync-spoke.sh",
    "postinstall": "yarn run build-spoke",
    "generate-certs": "cd scripts && bash generate-certs.sh"
  },
  "resolutions": {
    "is-promise": "2.1.0",
    "simple-swizzle/is-arrayish/is-promise": "2.1.0",
    "fs-capacitor/is-promise": "2.1.0"
  },
  "types": "lib/",
  "pre-push": [
    "test"
  ],
  "pre-commit": [
    "test",
    "precommit"
  ],
