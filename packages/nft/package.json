  "scripts": {
    "dev": "next dev --port 3001 --hostname 127.0.0.1",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=6144 next build ./ && yarn run export",
    "export": "next export -o www/ && mv www/404.html www/index.html && npx cap sync",
    "start": "next start",
    "watch": "cross-env NODE_ENV=development next --port 3001 --hostname 127.0.0.1",
    "validate": "yarn run lint && yarn run test",
    "test": "echo \"TODO: Add Testing to Client\" && exit 0",
    "lint": "eslint --fix --quiet \"*.ts\" \"**/*.ts\" \"**/*.tsx\"",
    "mint": "truffle exec scripts/mint.js",
    "populate": "truffle exec scripts/mintPopulateGallery.js",
    "populate:rinkeby": "truffle exec --network rinkeby scripts/mintPopulateGallery.js",
    "clean": "rm -rf build/contracts && rm -rf ./src/contracts && mkdir ./src/contracts",
    "copy-contracts": "cp -a ./build/contracts/. ./src/contracts",
    "migrate": "yarn clean && truffle migrate --network development --reset && yarn copy-contracts",
    "migrate:populate": "yarn clean && truffle migrate --network development --reset && yarn copy-contracts && yarn populate",
    "migrate:rinkeby": "yarn clean && truffle migrate --network rinkeby --reset && yarn copy-contracts"
  },
