    "start": "yarn run build && node dist/server.js",
    "dev": "cross-env NODE_ENV=development vite",
    "dev-reinit-db": "cross-env NODE_ENV=development FORCE_DB_REFRESH=true vite",
    "test": "echo \"TODO: no test specified\" && exit 0",
