{
  "name": "@xrengine/server",
  "description": "API Server for XREngine",
  "version": "0.3.25",
  "private": true,
  "homepage": "",
  "main": "dist/index.js",
  "keywords": [
    "metaverse",
    "three.js",
    "webgl",
    "mmo",
    "game engine",
    "webrtc",
    "ar",
    "xr",
    "vr"
  ],
  "author": {
    "name": "XREngine Collective",
    "email": "shawmakesmagic@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src/",
    "config": "config/"
  },
  "engines": {
    "node": ">= 16.3.0"
  },
  "jest": "ts-jest",
  "scripts": {
    "check-errors": "tsc --noemit",
    "dev": "concurrently \"cross-env NODE_ENV=development ts-eager src/index.ts\" \"cd ../gameserver && npm run dev\" \"npm run serve-local-files\"",
    "dev-windows": "concurrently -i \"cross-env NODE_ENV=development nodemon --watch src/** -r ts-eager/register src/index.ts\" \"cd ../gameserver && cross-env NODE_ENV=development nodemon --watch src/** -r ts-eager/register src/index.ts\" \"npm run serve-local-files\"",
    "start": "cross-env NODE_ENV=production ts-eager src/index.ts",
    "dev-api-server": "ts-eager src/index.ts",
    "dev-api-server-nossl": "cross-env NOSSL=true ts-eager src/index.ts",
    "dev-nossl": "concurrently \"cross-env NOSSL=true ts-eager src/index.ts\" \"cd ../gameserver && cross-env NOSSL=true ts-eager src/index.ts\"",
    "dev-reinit-db": "cross-env FORCE_DB_REFRESH=true ts-eager src/index.ts",
    "dev-reinit-db-windows": "concurrently -i \"cross-env NODE_ENV=development FORCE_DB_REFRESH=true nodemon --watch src/** -r ts-eager/register src/index.ts\" \"cd ../gameserver && cross-env NODE_ENV=development nodemon --watch src/** -r ts-eager/register src/index.ts\"",
    "test": "echo \"TODO: no test specified\" && exit 0",
    "lint": "eslint --fix --ext \"**/*.js\" \"**/*.ts\"",
    "validate": "npm run lint && npm run build && npm run test",
    "precommit": "no-master-commits -b master,dev",
    "upload-avatar": "node ./scripts/upload-avatar.js",
    "serve-local-files": "http-server ./upload --ssl --cert ../../certs/cert.pem --key ../../certs/key.pem --port 8642 --cors=*"
  },
  "types": "lib/",
  "resolutions": {
    "regenerator-runtime": "0.13.7",
    "socket.io": "4.1.2"
  },
