{
  "name": "@xr3ngine/native-plugin-xr",
  "version": "0.2.14",
  "description": "Native XR and camera services inside any capacitor app",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "yarn": ">= 0.18.0"
  },
  "npmClient": "yarn",
  "scripts": {
    "xlint": "yarn run prettier -- --check && yarn run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "yarn run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "yarn run build"
  },
  "author": "XR3 Group",
  "license": "MIT",
  "devDependencies": {
    "semantic-release": "17.4.2",
    "semantic-release-monorepo": "7.0.4",
    "@capacitor/android": "2.4.7",
    "@capacitor/core": "2.4.7",
    "@capacitor/ios": "2.4.7",
    "@ionic/prettier-config": "1.0.1",
    "@ionic/swiftlint-config": "1.1.2",
    "@rollup/plugin-node-resolve": "11.2.1",
    "prettier": "2.2.1",
    "prettier-plugin-java": "1.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.45.2",
    "swiftlint": "1.0.1",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.6"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorPluginAr.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xr3ngine/xr3ngine.git"
  },
  "bugs": {
    "url": "https://github.com/xr3ngine/xr3ngine/issues"
  },
  "homepage": "https://github.com/xr3ngine/xr3ngine#readme",
  "gitHead": "b69a8d4eb4d2b39bc013a8eba00b402b21932252"
}
